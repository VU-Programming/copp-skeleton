.main

.var
hello
world
.end-var

    BIPUSH 0x0
    BIPUSH 0x0
    BIPUSH 0x0
    BIPUSH 0X3
    BIPUSH 0X4
    ISTORE hello
    ISTORE world
    BIPUSH 0x2
    BIPUSH 0x3
    INVOKEVIRTUAL add
    BIPUSH 0x2
    HALT
.end-main

.method add(a, b)
      ILOAD a
      ILOAD b
      IADD
      IRETURN
.end-method

