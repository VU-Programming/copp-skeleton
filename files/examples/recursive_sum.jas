// 
// equivalent to:
// int sumem(int n) {
//     if (n == 0) return 0;
//     return n + times2(n-1);
// }
// sumem(10)
.constant
    objref  0xCAFE  // may be any value.  Needed by invokevirtual.
    COUNT 10
    .end-constant
    
.main

    LDC_W objref
    LDC_W COUNT
    INVOKEVIRTUAL sumem
    OUT // output '7', the ascii symbol at index 55
    HALT
.end-main
    
.method sumem(var)
    ILOAD var
    IFEQ ZERO
    ILOAD var
    LDC_W objref
    ILOAD var
    BIPUSH 0x1
    ISUB
    INVOKEVIRTUAL sumem
    IADD
    IRETURN
    ZERO:
        BIPUSH 0x0
        IRETURN
.end-method
    
